{
  "hosting":
  {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    {
      // Shows the same content for multiple URLs
      "source": "/product",
      "destination": "/product.html"
    }],
    "cleanUrls": true
  }
}